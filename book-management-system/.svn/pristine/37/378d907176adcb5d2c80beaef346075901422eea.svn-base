@CHARSET "gb2312";
*{
	margin: 0px auto;
	border: 0 ;
	padding:0px;
	font-family: Arial, Helvetica, sans-serif, "ו";
	font-size:12px;
}

#container {
width: 1024px;
margin: 0 auto;
}

.menu {
color: white;
padding-left:40px;
}
.menu1 {
color: white;
padding-right:15px;
}


.menu1 li {
color: white;
font-size: 12px;
float: right;
text-align: center;
padding: 0px 10px;
}
.menu1 li a{
	text-decoration: none;
	color:white;
	}
.menu ul, .menu1 ul, .menu li {
list-style-type: none;


}

tr {
display: table-row;
border-color: inherit; 
} 


ul {
list-style: none;
}
a{
	text-decoration: none;
	color:  #26466c;
} 

.menu li {
color: white;
font-size: 14px;
font-weight: bold;
text-align: center;
padding: 0px 20px;
float: left;
}
.menu li a{
	text-decoration: none;
	color:white;
	font-size: 12px;
	}

#content {
background-color:#ffffff;
width: 1024px;
height:auto;
margin: 0 auto;
}
.menu_box {
float:left;
text-decoration: none;
width:700px;
height:18px;
border-bottom: #AACBEE 1px solid;
padding:4px 10px 6px 17px;
margin-top:10px;
margin-bottom: 20px;
}
.menu_box ul li{
	float:left;
    list-style: none;
    width:300px;
}
.menu_box li.hover {
    padding: 0px;
    width: 300px;
    color: white;
    font-weight: bold;
    height: 23px;
}
#page{
	margin-bottom: 20px;
}
#page a{
	color: #ed7b22;
}
#time{
	float: right;
}
#right_content{
 	width:700px;
	height:auto;
	margin-left:80px;
	
}
p {
line-height:300%
}

#footer{
	width: 1024px;
	height:30px;
	margin: 0 auto;
	background-color: #2b82d1;
}